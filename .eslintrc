{
  "env": {
    "browser": false,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "eslint:recommended",
    "plugin:import/recommended"
  ],
  "plugins": [
    "import"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": [
      "never",
      "ignorePackages",
      {
        "js": "never"
      }
    ],
    "no-unused-vars": [
      "warn"
    ],
    "no-multiple-empty-lines": [
      "warn"
    ]
  }
}